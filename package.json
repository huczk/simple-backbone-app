{
  "name": "simple-backbone-app",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start-server\" \"npm run start-db\" \"npm run open\"",
    "start-server": "http-server -p 7777",
    "start-db": "json-server --watch db.json",
    "open": "start http://localhost:7777"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "http-server": "^0.10.0",
    "json-server": "^0.12.0"
  }
}
